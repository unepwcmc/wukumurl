<section>
  <header>
    <h4><%= title %></h4>
  </header>

  <div class="multi-row-table-wrapper">
    <table>
      <tbody>
      <% @short_urls.each_with_index do |short_url, index| %>
        <tr>
          <td>
            <%= index + 1 %>. <%= link_to short_url.short_name , 
              short_url_info_path(short_url.short_name) %>
          </td>
          <td class="right time">
            <%= short_url.created_at.strftime("%d %b. %Y") %>
          </td>
        </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</section>
