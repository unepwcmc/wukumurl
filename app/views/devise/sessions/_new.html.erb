<%= form_for("user", :url => user_session_path) do |f| %>
  <div>
    <%= f.email_field :email, :autofocus => true, :placeholder => "Enter your WCMC email address" %>
  </div>

  <div>
  <%= f.password_field :password, :placeholder => "Enter your WCMC password" %></div>

  <div id="remember"><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>

  <div id="sign_button"><%= f.submit "Sign in" %></div>
<% end %>

